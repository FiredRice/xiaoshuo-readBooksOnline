{
  "name": "xiaoshuo-custom",
  "displayName": "xiaoshuo-custom",
  "description": "后面改",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xiaoshuo-custom.search",
        "title": "搜索"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jiege-sidebar",
          "title": "祖安钢琴师",
          "icon": "static/images/icon.svg"
        }
      ]
    },
    "views": {
      "jiege-sidebar": [
        {
          "id": "jiege-sidebar-bookTree",
          "name": "列表"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "xiaoshuo-custom.search",
          "when": "view == jiege-sidebar-bookTree",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "cheerio": "^1.0.0-rc.12"
  }
}
